        android:textColor="@color/black" />

    <TextView
        android:id="@+id/tvw5"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="基座-tvw5-反射资源"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/tvw6"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
